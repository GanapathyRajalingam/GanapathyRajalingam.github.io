<dom-module id="my-d3chart">

  <script>
    (function() {
      
      Polymer({
        is: 'my-d3chart',
        nvModel: nv.models.lineChart(),
        behaviors: [NVD3Behavior],
        onCreate: function(chart, svg) {
          console.log('on create called in my chart');
            chart.useInteractiveGuideline(true);
          },

              ready: function(){
          //      this.spotarr = [];
               console.log('ready fn called for my-d3chart');
              },

              properties: {
    data: {
        type: Object,
        value: function() {
            return [
                {
                    'key': 'Line 1',
                    'values': [
                        {'x': 0, 'y': 1},
                        {'x': 1, 'y': 2},
                        {'x': 2, 'y': 3}
                    ]
                }
            ];
        }
    }
},
      });
    })();
  </script>

</dom-module>
